{
  "version": "0.3.7",
  "name": "@kmamal/sdl",
  "description": "SDL2 bindings for Node.js",
  "keywords": [
    "sdl",
    "sdl2",
    "gui",
    "windows",
    "screen",
    "video",
    "audio",
    "sound",
    "input",
    "mouse",
    "keyboard",
    "clipboard",
    "game"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kmamal/node-sdl.git"
  },
  "main": "src/javascript/index.js",
  "types": "src/types/index.d.ts",
  "gypfile": true,
  "scripts": {
    "test": "tests/run-all.sh",
    "rebuild": "node-gyp rebuild --verbose",
    "prebuild": "prebuild --runtime napi --include-regex \"\\.(node|dll|dylib|so\\.0)$\" --verbose",
    "install": "node-gyp rebuild --verbose"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "prebuild-install": "^7.1.1"
  },
  "devDependencies": {
    "prebuild": "^11.0.3"
  }
}
